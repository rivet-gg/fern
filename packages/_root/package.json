{
  "name": "@fern-api/compile-root",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/fern-api/fern.git",
    "directory": "packages/_root"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "yarn compile && jest --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-path=../../.eslintignore --report-unused-disable-directives",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../shared/.prettierignore \"**\"",
    "organize-imports": "organize-imports-cli tsconfig.json",
    "depcheck": ":"
  },
  "dependencies": {
    "@fern-api/auth": "workspace:*",
    "@fern-api/cli": "workspace:*",
    "@fern-api/config-management-commons": "workspace:*",
    "@fern-api/core": "workspace:*",
    "@fern-api/core-utils": "workspace:*",
    "@fern-api/dependencies-configuration": "workspace:*",
    "@fern-api/docker-utils": "workspace:*",
    "@fern-api/docs-configuration": "workspace:*",
    "@fern-api/docs-preview": "workspace:*",
    "@fern-api/docs-validator": "workspace:*",
    "@fern-api/ete-tests": "workspace:*",
    "@fern-api/fs-utils": "workspace:*",
    "@fern-api/generators-configuration": "workspace:*",
    "@fern-api/init": "workspace:*",
    "@fern-api/ir-generator": "workspace:*",
    "@fern-api/ir-migrations": "workspace:*",
    "@fern-api/json-schema": "workspace:*",
    "@fern-api/loadable": "workspace:*",
    "@fern-api/local-workspace-runner": "workspace:*",
    "@fern-api/logger": "workspace:*",
    "@fern-api/logging-execa": "workspace:*",
    "@fern-api/login": "workspace:*",
    "@fern-api/manage-generator": "workspace:*",
    "@fern-api/openapi-ir-to-fern": "workspace:*",
    "@fern-api/openapi-parser": "workspace:*",
    "@fern-api/posthog-manager": "workspace:*",
    "@fern-api/project-configuration": "workspace:*",
    "@fern-api/project-loader": "workspace:*",
    "@fern-api/register": "workspace:*",
    "@fern-api/remote-workspace-runner": "workspace:*",
    "@fern-api/scripts": "workspace:*",
    "@fern-api/seed-cli": "workspace:*",
    "@fern-api/semver-utils": "workspace:*",
    "@fern-api/task-context": "workspace:*",
    "@fern-api/validator": "workspace:*",
    "@fern-api/workspace-loader": "workspace:*",
    "@fern-api/yaml-formatter": "workspace:*",
    "@fern-api/yaml-migrations": "workspace:*",
    "@fern-api/yaml-schema": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "depcheck": "^1.4.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "organize-imports-cli": "^0.10.0",
    "prettier": "^2.7.1",
    "typescript": "4.6.4"
  }
}
