                "request": {
                  "attributes": {
                    "allowed_locations": [
                      "US",
                      "CA",
                    ],
                  },
                  "key": "stripeinc",
                  "name": "Stripe Inc",
                },
                "response": {
                  "body": {
                    "attributes": {
                      "allowed_locations": [
                        "US",
                        "CA",
                      ],
                    },
                    "created_at": "2019-08-24T14:15:22Z",
                    "description": "description",
                    "environment_id": "40ef0e48-a11f-4963-a229-e396c9f7e7c4",
