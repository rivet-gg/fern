{
  "type": "cli",
  "cliName": "fern",
  "environment": {
    "environments": {
      "prod": {
        "cliName": "fern",
        "cliPackageName": "fern-api"
      },
      "dev": {
        "cliName": "fern-dev",
        "cliPackageName": "@fern-api/fern-api-dev"
      },
      "local": {
        "cliName": "fern-local",
        "cliPackageName": "@fern-api/fern-api-local"
      }
    },
    "variables": [
      "AUTH0_DOMAIN",
      "AUTH0_CLIENT_ID",
      "DEFAULT_FIDDLE_ORIGIN",
      "DEFAULT_VENUS_ORIGIN",
      "DEFAULT_FDR_ORIGIN",
      "VENUS_AUDIENCE",
      "LOCAL_STORAGE_FOLDER",
      "POSTHOG_API_KEY",
      "DOCS_DOMAIN_SUFFIX",
      "DOCS_PREVIEW_BUCKET"
    ]
  },
  "plugins": {
    "jsoncParserResolverPlugin": "./jsoncParserResolverPlugin.cjs"
  },
  "private": true
}
